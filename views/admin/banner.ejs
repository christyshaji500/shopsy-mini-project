  <div class="container-xxl flex-grow-1 container-p-y">
      <h4 class="fw-bold py-3 mb-4"><span class="text-muted fw-light"></span> </h4>
      <form action="/admin/addBanner" method="post" enctype="multipart/form-data">
        <div class="row">
            <div class="col-md-12 mb-4 d-flex  justify-content-around">
                <div class="form-outline">
                    <input style="width: 30vw;" type="text" name="banner" id="form3Example1"
                        class="form-control" placeholder="Banner name" />
                </div>
    
                <% if(typeof message !=='undefined' ){%>
    
                    <p class="mt-1 ps-2 text-danger">
                        <%= message %>
                    </p>
    
                    <%}%>
                        <div class="form-outline ">
                            <input style="width: 30vw;" type="file" name="bannerImage"
                                id="form3Example1" class="form-control" multiple />
                        </div>
    
                        <input style="width:7vw; " type="submit"
                            class="btn btn-primary   form-control " value="Upload">
            </div>
        </div>
    
    </form>
      

<div class="card">
<h5 class="card-header">Banner list</h5>
<div class="table-responsive text-nowrap">
<table class="table">
<thead>

</thead>
<thead>
    <th><span>Sl NO</span></th>
    <th><span>Image</span></th>
    <th><span>Name</span></th>
    <th>Status</th>
    <th><span>Actions</span></th>
</thead>
<tbody>
    <% if(banners.length> 0){
        for(let i=0; i<banners.length;i++ ){%>
  <tr>
   
      <td>
        <span>
            <%=i+1 %>
        </span>
          
      </td>
      <td style="image-resolution: 5px;">
         <img src="/admin/multer/img/<%=banners[i].bannerImage[0] %> "
             alt="" width="100vw" height="50vh">
         <img src="/admin/multer/img/<%=banners[i].bannerImage[1] %>"
             alt="" width="100vw" height="50vh">
         <img src="/admin/multer/img/<%=banners[i].bannerImage[2] %>"
             alt="" width="100vw" height="50vh">
      </td>
      <td>
        <%=banners[i].banner %>
      </td>
      <td>
        
        <% if(banners[i].is_active==1) { %>
                <td><span>Is active</span></td>
        <% } else { %>
            <td><span>In rest</span></td>
            <% } %>
        
      </td>
      <td>
        <a href="/admin/activeBanner?id=<%= banners[i]._id %>" class="btn btn-info"> choose </a>
      </td>
    <td>
        <%}}%>
             
      </td>
      
  
</tbody>
</table>
</div>
</div>



